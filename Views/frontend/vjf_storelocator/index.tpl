{extends file="parent:frontend/index/index.tpl"}
{block name="frontend_index_header_javascript_jquery_lib" append}
    <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD2JhlG7h2qoz01_dXSepPcCT5zdYtKc4E&callback=initMap" type="text/javascript"></script>
    <script type="text/javascript">// <![CDATA[
    	(function($){
          // $(window).load(function(){
          //     $("#jlocator .panel").mCustomScrollbar();
          // });
    	})(jQuery);

      function initMap() {
        $('#jlocator').jlocator();
        $('#jlocator .paging.left').prev('.clear').remove();
        setTimeout("$('#jlocator .search-within-radius .autocomplete').focus()", 300);
      }
    // ]]>
    </script>
{/block}
{block name="frontend_index_content_top"}{/block}

{block name="frontend_index_content"}
<div class="std"><div id="jlocator">
  <input type="hidden" name="icon_marker" value="{link file="frontend/_public/src/img/marker.png"}" />
  <input type="hidden" name="icon_directions" value="{link file="frontend/_public/src/img/directions.png"}" />
<div class="panel" data-type="panel">
<div class="controls search-within-radius" data-type="controls">
<div class="box">
<div class="filter left"><input type="text" data-path="p" data-control-type="textbox" data-control-name="title-filter" data-control-action="filter"></div>
</div>
<div class="box">
<div class="paging-results left" data-control-type="label" data-control-name="paging" data-control-action="paging" data-type="##retailer_paging##">##retailer_paging##</div>

<div class="paging left" style="display: none;" data-control-type="placeholder" data-control-name="paging" data-control-action="paging" data-items-per-page="999">
  <div class="pagingprev hidden">
    <span class="first" data-number="0">&lt;&lt;</span>
    <span class="prev" data-number="0">&lt;</span>
  </div>
  <div class="pagingmid">
    <div class="pagesbox">
      <span class="current" data-active="true" data-number="0">1</span>
    </div></div><div class="pagingnext hidden">
      <span class="next" data-number="0">&gt;</span>
      <span class="last" data-number="0">&gt;&gt;</span>
    </div>
  </div>
</div>
</div>
<div class="stores box directions" data-type="stores">
  {foreach $vjfStores as $vjfStore}
      {include file="frontend/vjf_storelocator/store.tpl"}
  {/foreach}
</div>
</div>
